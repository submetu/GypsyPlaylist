{
  "author": "Subhan",
  "name": "gypsy-playlist-frontend",
  "version": "0.1.0",
  "dependencies": {
    "@material-ui/core": "^4.0.1",
    "@material-ui/icons": "^4.0.1",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.16",
    "@types/material-ui": "^0.21.6",
    "@types/node": "^12.6.9",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "@types/react-router-dom": "^4.3.4",
    "@types/shortid": "0.0.29",
    "classnames": "^2.2.6",
    "querystring": "~0.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "4.1.1",
    "react-scripts": "^3.0.1",
    "shortid": "^2.2.14",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^8.0.8",
    "@types/fetch-mock": "^7.3.1",
    "fetch-mock": "^7.3.9",
    "husky": "^3.0.2",
    "lint-staged": "^9.2.1",
    "node-fetch": "^2.6.1",
    "prettier": "^1.18.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:bail": "yarn test --watchAll=false --bail",
    "eject": "react-scripts eject",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx,js}' --fix --format verbose",
    "prettier": "./node_modules/prettier/bin-prettier.js --check --config ./.prettierrc 'src/**/*' --write",
    "travis:test": "yarn lint && yarn prettier && yarn test:bail",
    "travis:build": "yarn lint && yarn prettier && yarn test && yarn build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test:bail"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "yarn prettier",
      "git add"
    ]
  },
  "proxy": "http://localhost:8888"
}
